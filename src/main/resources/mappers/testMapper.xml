<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 <mapper namespace="com.givemetreat.test.mapper.testMapper">
 	<select id="getTestFirstDummy" resultType="com.givemetreat.product.domain.Product" >
	SELECT
		`id`
		, `name`
		, `category`
		, `price`
		, `agePetProper`
		, `imgProfile`
		, `imgThumbnail`
		, `createdAt`
		, `updatedAt`
	FROM
		`product`
	WHERE
		`id` = 1	 	
 	</select>
 </mapper>